#环境标识：开发环境=DEV,测试环境=TEST,预发布环境=PRE,正式环境=PRD
env = PRD

#开发模式：输出JFinal日志
devMode = true
#显示SQL输出
showSql = true
#默认事务级别配置(MYSQL=4,Oralce=2)
transaction_level = 4

#系统启动时根据当前Eova数据源生成Oracle SQL
initSql = false
#系统启动时自动下载js插件包
initPlugins = false

#是否开启升级控制台
isUpgrade = false
#是否启用定时任务
isQuartz = true

#默认数据源字典表名
main_dict_table = dicts
#默认(逻辑删除/隐藏)的字段名
hide_field_name = is_hide

#是否校验验证码(false按回车就能登录)
isCaptcha = true

#超级管理员角色ID
admin_rid = 1

#文件框上传文件后缀白名单
upload_type = .jpg|.jepg|.gif|.png|.bmp|.gz|.7z|.rar|.zip|.swf|.mp3|.mp4|.jar|.apk|.ipa|.doc|.docx|.xls|.xlsx|.ppt|.pptx|.pdf|.txt
